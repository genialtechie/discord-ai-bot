    "@elizaos/core": "0.1.7",
    "@elizaos/plugin-bootstrap": "0.1.7",
    "@elizaos/plugin-node": "0.1.7",
    "@supabase/supabase-js": "^2.39.3",
    "@tavily/core": "^0.0.2",
    "amqplib": "0.10.5",
    "dotenv": "^16.4.1",
    "fs": "^0.0.1-security",
    "net": "^1.0.2",
    "path": "^0.12.7",
